RewriteEngine On
RewriteCond %{HTTPS} off [OR]
RewriteCond %{HTTP_HOST} ^www\. [NC]
RewriteCond %{HTTP_HOST} ^(?:www\.)?(.+)$ [NC]
RewriteRule ^ https://%1%{REQUEST_URI} [L,NE,R=301]


#RewriteRule ^(.*)$ http://www.groupwayindia.com$1 [r=301,nc]
ErrorDocument 404 https://www.marketingfundas.com

